<script> 
    (function() {
        const offcanvasContainer = document.querySelector('.js-author-offcanvas-container')

        const backdrop = document.createElement('div');
        backdrop.className = 'js-rea-module-author_offcanvas-backdrop';
        // const backdrop = document.querySelector('.js-author-offcanvas-backdrop')
        
        const offcanvasOuter = document.querySelector('.rea-module-author_offcanvas')

        offcanvasOuter.after(backdrop);

        const offcanvas = document.querySelector('.js-author-offcanvas')
        const trigger = document.querySelector('.js-author-drawer-toggle')
        const closer = document.querySelector('.js-author-offcanvas-close')
        const width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
        const guideBody = document.querySelector('.rea-container');
        const articleBody = document.querySelector('.single-artikel');
        let isIn = false;

        offcanvas.classList.remove('hidden');

        if (width >= 1024) {
            const viewportOffset = offcanvasContainer.getBoundingClientRect();
            const left = viewportOffset.left;
            const elementWidth = width - left;

            offcanvasOuter.style.width = elementWidth + 'px';
            offcanvasOuter.style.display = 'block';
        }

        function toggleOffCanvas(ev) {
            ev.preventDefault();
            offcanvasOuter.classList.toggle('in')
            backdrop.classList.toggle('in')
            isIn = !isIn;
            if(width < 1024) {
                if (isIn) {
                    if(guideBody) {
                        guideBody.style.zIndex = '1009';
                    }
                    if(articleBody) {
                        articleBody.style.zIndex = '1007';
                    }
                    document.body.style.overflow = 'hidden';
                } else {
                    if(guideBody) {
                        guideBody.style.removeProperty('z-index');
                    }
                    if(articleBody) {
                        articleBody.style.removeProperty('z-index');
                    }
                    document.body.style.removeProperty('overflow');
                }
            }
        }

        trigger.addEventListener('click', toggleOffCanvas)
        backdrop.addEventListener('click', toggleOffCanvas)
        closer.addEventListener('click', toggleOffCanvas)
    })();
