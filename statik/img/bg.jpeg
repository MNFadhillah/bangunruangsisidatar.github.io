        skeletonLoading.style.display = 'none';
    }

    function fetchMorePosts(currentPage) {
        const fetchUrl = `/wp-json/wp/v2/posts?author=165&cat=&post_status=publish&per_page=9&page=${currentPage}&_embed`;

        fetch(fetchUrl)
            .then((response) => response.json())
            .then((data) => {
                if (currentPage === 226) {
                    buttonContainer.style.display = 'none';
                } else {
                    buttonContainer.style.display = 'block';
                }
                if (data.length > 0) {
                    const authorCardContainer = document.querySelector('.author-card-container');

                    data.forEach(({
                        modified,
                        link,
                        title: {
                            rendered: title
                        },
                        excerpt: {
                            rendered: excerpt
                        },
                        content: {
                            rendered: content
                        },
                        authorName,
                        _embedded: {
                            'wp:featuredmedia': featuredmedia
                        }
                    }, i) => {
                        const imgMatch = content.match(/<img.+?src=(["'])(.+?)\1/);

                        let imageSrc = imgMatch ? imgMatch[2] : '';

                        const relativeLink = new URL(link);
                        const authorCard = document.createElement('div');
                        authorCard.className = 'rea-module rea-module-author_card_list';

                        const imgElement = document.createElement('img');
                        imgElement.alt = title;
                        imgElement.decoding = 'async';
                        imgElement.loading = 'lazy';
                        imgElement.addEventListener('error', function() {
                            imgElement.src = 'https://www.rumah123.com/asset-core/images/image-guide-placeholder.webp';
                        });

                        imgElement.src = imageSrc;

                        authorCard.innerHTML = `
                <a class="author-card" href="${relativeLink.pathname}" title="${title}" style="opacity: 0; animation: grow 200ms ${i}00ms forwards">
                    <div class='img'></div>
                    <div class="author-card__content">
                        <time datetime="${modified}">${timestampToIndonesianDate(modified)}</time>
                        <h3>${title}</h3>
                        ${excerpt}
                        <div class="author-card__content__info">
                            <span>
                                <svg><use xlink:href="#icon-user" /></svg>
                                ${authorName}
                            </span>
                            <span>
                                <svg><use xlink:href="#icon-time" /></svg>
                                ${postContentToReadMinute(content)} min read
                            </span>
                        </div>
                    </div>
                </a>`;

                        authorCard.querySelector('.img').appendChild(imgElement);
                        authorCardContainer.appendChild(authorCard);
                        hideSkeletonLoading();
                    });
                    page++;
                    loadingPosts = false;
                } else {
                    buttonContainer.style.display = 'none';
                }
            });
    }

    const changeCategory = (e, c) => {
        e.preventDefault();
        const cardList = document.querySelector('.rea-module-author_card_list');
        const authorTabs = document.querySelectorAll('.author-tabs__tab');
        const spinner = document.createElement('div');
        spinner.className = 'spinner-container';

        authorTabs.forEach((e) => {
            e.classList.remove('active-tab');
            e.setAttribute('style', 'pointer-events: none;');
        });
        e.target.classList.add('active-tab');
        cardList.querySelectorAll('.author-card').forEach((oldcard) => {
            oldcard.querySelector('time').classList.add('spinner-container');
            oldcard.querySelectorAll('span').forEach(s => s.classList.add('spinner-container'));
            oldcard.querySelector('h3').classList.add('spinner-container');
            oldcard.querySelector('p').classList.add('spinner-container');
            oldcard.querySelector('.img').classList.add('spinner-container');
            oldcard.addEventListener('click', (elm) => elm.preventDefault());
        });

        fetch(`/wp-json/wp/v2/posts?author=165${c ? `&categories=${c}` : ''}&_embed`)
            .then((response) => response.json())
            .then((data) => {
                cardList.innerHTML = '';
                authorTabs.forEach((e) => e.removeAttribute('style'));

                data.forEach(({
                    modified,
                    link,
                    title: {
                        rendered: title
                    },
                    excerpt: {
                        rendered: excerpt
                    },
                    content: {
                        rendered: content
                    },
                    authorName,
                    _embedded: {
                        'wp:featuredmedia': featuredmedia
                    }
                }, i) => {
                    const image = featuredmedia.length ? featuredmedia?.[0]?.media_details?.sizes?.['post-thumbnail']?.source_url : '';
                    const relativeLink = new URL(link);

                    cardList.innerHTML += `
                   <a class="author-card" href="${relativeLink.pathname}" title="${title}" style="opacity: 0;animation: grow 200ms ${i}00ms forwards">
                        <div class='img'><img src="${image}" alt="${title}" decoding="async" loading="lazy"></div>
                        <div class="author-card__content">
                            <time datetime="${modified}">${timestampToIndonesianDate(modified)}</time>
                            <h3>${title}</h3>
                            ${excerpt}
                            <div class="author-card__content__info">
                                <span>
                                    <svg><use xlink:href="#icon-user" /></svg>
                                    ${authorName}
                                </span>
                                <span>
                                    <svg><use xlink:href="#icon-time" /></svg>
                                    ${postContentToReadMinute(content)} min read
                                </span>
                            </div>
                        </div>
                    </a>`
                });
            });
    }

    const timestampToIndonesianDate = (timestamp) => {
        const date = new Date(timestamp);
        const month = [
            'Januari', 'Februari', 'Maret', 'April', 'Mei', 'Juni', 'Juli', 'Agustus', 'September', 'Oktober', 'Nopember', 'Desember'
        ];

        return `${date.getDate()} ${month[date.getMonth()]} ${date.getFullYear()}`;
    }

    const postContentToReadMinute = (postContent) => {
        const dummy = document.createElement('div');
        dummy.innerHTML = postContent;
        const wordCount = dummy.innerText.split(' ').length;

        return Math.ceil(wordCount / 200);
    }
</script>
